document.addEventListener('DOMContentLoaded', function(e) {
	// everything
	var num = 5;		// Number	
	var number = '5';	// String
	alert(num == number);	// true, loosely-typed
	alert(num === number);	// false, checking data type
	
}, false);
</script>
