<script type="text/javascript">
// trainee constructor
function Trainee(id,name,major) {
	this.id = id;
	this.name = name;
	this.major = major;
}
// get trainee from form
function process(){
	var trainee = 
		new Trainee( $('#id').val(), $('#name').val(), $('#major').val() );
	console.log(trainee);
}
</script>
