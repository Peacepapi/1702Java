<script type="text/javascript">
$(document).ready(function() { // DOMContentLoaded
	$('div').css('backgroundColor', 'blue'); // change css prop
	$('#dan').html('<h1>Pound Dan</h1>'); //innerHTML
	$('.pickles').text('Pickles'); // getelementsbyclassname / innerText
	
	$('button').click(function(e) {   // addEventListener
		
	});
	
});
</script>
